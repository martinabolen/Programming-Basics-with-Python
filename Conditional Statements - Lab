2.
a = int(input())
b = int(input())

if a > b:
    print(a)
else:
    print(b)


3.
number = int(input())

if number % 2 == 0:
    print('even')
else:
    print('odd')
  
4.
number = int(input())

if number == 1:
    print('one')
elif number == 2:
    print('two')
elif number == 3:
    print('three')
elif number == 4:
    print('four')
elif number == 5:
    print('five')
elif number == 6:
    print('six')
elif number == 7:
    print('seven')
elif number == 8:
    print('eight')
elif number == 9:
    print('nine')
else:
    print('number too big')
    
    
5.
number = int(input())

if number < 100:
    print('Less than 100')
elif number <= 200:
    print('Between 100 and 200')
else:
    print('Greater than 200')
    
